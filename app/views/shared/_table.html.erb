<table class="styled-table">
    <thead>
        <tr>
            <th>#id</th>
            <th>Activity</th>
            <th>Content</th>
            <%# <th>Robot model</th> %>
            <th>Booking date</th>
        </tr>
    </thead>
    <tbody>
      <% user_bookings.each do |b| %>
        <tr>
            <td><%= b.id %></td>
            <td><%= b.activity %></td>
            <td><%= b.comment %></td>
            <td><%= b.created_at %></td>
        </tr>
      <% end %>
    </tbody>
</table>
